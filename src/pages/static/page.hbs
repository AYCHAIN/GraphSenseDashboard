<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8"/>
    <title>{{htmlWebpackPlugin.options.title}}</title>
    {{!-- css variable is filled by static.js --}}
    {{!-- for landingpage the css is injected by HtmlWebpackPlugin --}}
    {{#each css }}
    <link href="{{file}}" rel="stylesheet">
    {{/each}}
  </head>
  <body>
    <div class="{{htmlWebpackPlugin.options.staticpage_classes}} static-page">
      {{{htmlWebpackPlugin.options.header}}}
      {{{htmlWebpackPlugin.options.page}}}
      {{{htmlWebpackPlugin.options.footer}}}
    </div>
  </body>
</html>
